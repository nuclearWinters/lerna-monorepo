    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-relay": "^16.0.6",
    "@types/relay-runtime": "^17.0.3",
    "babel-jest": "^29.0.0",
    "babel-loader": "^9.1.0",
