{
  "name": "backend-auth",
  "version": "1.0.1",
  "description": "Backend typescript express relay mongodb",
  "main": "src/exports.ts",
  "scripts": {
    "start": "ts-node-dev --transpile-only ./src/index.ts",
    "populate": "npx tsc && node ./build/populate.js",
    "build": "npx tsc",
    "serve": "node ./build/index.js",
    "pcm": "lint-staged",
    "test": "jest --bail",
    "proto": "sh src/proto/build.sh"
  },
  "author": "Armando Rueda",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.8.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.1",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-helix": "^1.13.0",
    "graphql-relay": "^0.10.0",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^3.3.4",
    "node-device-detector": "^2.0.10",
    "redis": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@shelf/jest-mongodb": "^4.1.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-jest": "^29.0.0",
    "eslint": "^8.30.0",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.12.3",
    "jest": "^29.0.0",
    "lint-staged": "^13.1.0",
    "mongodb": "^4.13.0",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "**/!(*_pb).{js,ts}": [
      "eslint --cache --fix",
      "prettier --write",
      "jest --findRelatedTests --bail --passWithNoTests"
    ]
  }
}
