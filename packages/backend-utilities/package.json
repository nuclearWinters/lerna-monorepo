{
  "name": "@lerna-monorepo/backend-utilities",
  "version": "1.0.1",
  "description": "Backend typescript express relay mongodb",
  "exports": {
    "./config": {
      "types": "./src/config.ts",
      "default": "./build/config.js"
    },
    "./index": {
      "types": "./src/index.ts",
      "default": "./build/index.js"
    },
    "./types": {
      "types": "./src/types.ts",
      "default": "./build/types.js"
    },
    "./grpc": {
      "types": "./src/grpc.ts",
      "default": "./build/grpc.js"
    },
    "./protoAuth/auth_grpc_pb": {
      "types": "./src/protoAuth/auth_grpc_pb.d.cts",
      "default": "./build/protoAuth/auth_grpc_pb.cjs"
    },
    "./protoAuth/auth_pb": {
      "types": "./src/protoAuth/auth_pb.d.cts",
      "default": "./build/protoAuth/auth_pb.cjs"
    },
    "./protoAccount/account_grpc_pb": {
      "types": "./src/protoAccount/account_grpc_pb.d.cts",
      "default": "./build/protoAccount/account_grpc_pb.cjs"
    },
    "./protoAccount/account_pb": {
      "types": "./src/protoAccount/account_pb.d.cts",
      "default": "./build/protoAccount/account_pb.cjs"
    }
  },
  "type": "module",
  "scripts": {
    "start": "ts-node-dev --transpile-only ./src/index.ts",
    "populate": "npx tsc && node ./build/populate.js",
    "build": "npx tsc && node ./copyCode.mjs",
    "serve": "node ./build/index.js",
    "pcm": "lint-staged",
    "test": "jest --bail",
    "protoAuth": "grpc_tools_node_protoc --plugin=protoc-gen-ts=../../node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:src/protoAuth --proto_path=src/protoAuth --js_out=import_style=commonjs,binary:src/protoAuth --grpc_out=grpc_js:src/protoAuth ./src/protoAuth/*.proto",
    "protoAccount": "grpc_tools_node_protoc --plugin=protoc-gen-ts=../../node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:src/protoAccount --proto_path=src/protoAccount --js_out=import_style=commonjs,binary:src/protoAccount --grpc_out=grpc_js:src/protoAccount ./src/protoAccount/*.proto",
    "proto": "npm run protoAuth && npm run protoAccount"
  },
  "author": "Armando Rueda",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.11.1",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.6.0",
    "date-fns": "^3.0.6",
    "graphql": "^16.5.0",
    "graphql-relay": "^0.10.2",
    "graphql-sse": "^2.5.3",
    "jsonwebtoken": "^9.0.0",
    "redis": "^4.6.15"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "@babel/preset-typescript": "^7.18.6",
    "@eslint/js": "^9.9.1",
    "@shelf/jest-mongodb": "^4.3.2",
    "@testcontainers/redis": "^10.11.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.6.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/eslint__js": "^8.42.3",
    "@types/google-protobuf": "^3.15.6",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/supertest": "^6.0.1",
    "babel-jest": "^29.0.0",
    "eslint": "^9.9.1",
    "grpc_tools_node_protoc_ts": "^5.3.2",
    "grpc-tools": "^1.12.3",
    "jest": "^29.0.0",
    "lint-staged": "^15.0.2",
    "mongodb": "^6.8.0",
    "prettier": "^3.0.3",
    "supertest": "^7.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0"
  },
  "lint-staged": {
    "**/!(*_pb).{js,ts}": [
      "eslint --cache --fix",
      "prettier --write",
      "jest --findRelatedTests --bail --passWithNoTests"
    ]
  }
}
